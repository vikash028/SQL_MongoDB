create database VIT;
drop database VIT;
use ViT;
create table CSE(
s_id int,
s_name varchar(30),
s_mark int
);
drop table cse;
show tables from VIt;

SELECT 
    *
FROM
    cse;
insert into cse values (1001,'Ram Prasad',99);

ALTER TABLE cse RENAME column 
    s_country TO s_state;
desc cse;
select * from cse;

delete from cse where s_id=1002;

